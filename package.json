{
  "name": "SubQuery-Demo",
  "version": "0.0.5",
  "description": "This project can be used as a starting point for developing your SubQuery project. It indexes all transfers on Litentry network",
  "main": "dist/index.js",
  "scripts": {
    "build": "subql codegen && subql build",
    "codegen": "subql codegen",
    "start:docker": "docker-compose pull && docker-compose up --remove-orphans",
    "dev": "subql codegen && subql build && docker-compose pull && docker-compose up --remove-orphans",
    "prepack": "rm -rf dist && npm run build",
    "test": "subql build && subql-node test",
    "build:develop": "NODE_ENV=develop subql codegen && NODE_ENV=develop subql build"
  },
  "homepage": "https://github.com/subquery/subql-starter",
  "repository": "github:subquery/subql-starter",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "SubQuery Team",
  "license": "MIT",
  "devDependencies": {
    "@polkadot/api": "^12",
    "@subql/cli": "latest",
    "@subql/node": "latest",
    "@subql/testing": "latest",
    "@subql/types": "latest",
    "dotenv": "latest",
    "typescript": "^5.2.2"
  },
  "exports": {
    "chaintypes": "src/chaintypes.ts"
  },
  "dependencies": {
    "@subql/types-core": "^2.0.0"
  }
}
